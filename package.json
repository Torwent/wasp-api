    "bcryptjs": "^2.4.3",
    "discord.js": "^14.15.3",
    "elysia": "latest",
    "elysia-autoload": "^1.0.1",
    "elysia-autoroutes": "^0.5.0",
    "elysia-rate-limit": "^4.1.0",
    "logestic": "^1.2.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "bun-types": "^1.1.15",
    "prettier": "^3.3.2"
